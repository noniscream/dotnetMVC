       }
     li a{
         margin-right:2rem;
         text-decoration:none;
     }
     .item1::after {
         content: '';
         display: block;
         width: 0;
         height: 2.5px;
         background: #69491A;
         transition: width .3s;
         box-shadow: 8px 8px 8px;
     }
     .item1:hover::after {
         width: 100%;
         transition: width .3s;
         text-align: right;
     }
